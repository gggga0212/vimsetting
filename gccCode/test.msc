/* Periodic Sync 8.2.3*/
msc{
    hscale="3";
    App, BLE_LIB, Scanner;

    Scanner => BLE_LIB [label="BLE_GAP_SetAdvertisingParams"];
    BLE_LIB >> Scanner[label="MBA_RES_SUCCESS"];

    Scanner => BLE_LIB [label="BLE_GAP_SetExtScanningEnable"];
    BLE_LIB >> Scanner[label="MBA_RES_SUCCESS"];

    App => BLE_LIB [label="ADV_EXT_IND"];
    BLE_LIB >> App[label="MBA_RES_SUCCESS"];

    App => BLE_LIB [label="ADV_EXT_IND"];
    BLE_LIB >> App[label="MBA_RES_SUCCESS"];

    App => BLE_LIB [label="ADV_EXT_IND"];
    BLE_LIB >> App[label="MBA_RES_SUCCESS"];

    App => BLE_LIB [label="ADV_EXT_IND"];
    BLE_LIB >> App[label="MBA_RES_SUCCESS"];

    App => BLE_LIB [label="ADV_SYNC_IND"];
    BLE_LIB >> App[label="MBA_RES_SUCCESS"];

    BLE_LIB => Scanner [label="BLE_GAP_EVT_ADV_REPORT"];

    Scanner => BLE_LIB [label="BLE_GAP_CreateSync"];
    BLE_LIB >> Scanner[label="MBA_RES_SUCCESS"];

    BLE_LIB => Scanner [label="BLE_GAP_EVT_PERI_ADV_SYNC_EST"];
    BLE_LIB => Scanner [label="BLE_GAP_EVT_PERI_ADV_REPORT"];
}



